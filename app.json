{
  "expo": {
    "name": "vinime-app",
    "slug": "vinime-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "vinimeapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#180042"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.victorbreno.vinimeapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#180042"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.victorbreno.vinimeapp"
    },
    "plugins": [
      "expo-font"
    ]

  }
}
